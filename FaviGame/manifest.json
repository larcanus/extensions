{
  "author":"Alex Rulkov",
  "name":"FaviGame",
  "description":"Collect favicon in space!",
  "version":"1.0",
  "manifest_version":3,
  "background":{
    "service_worker":"background.js",
    "type":"module"
  },
  "permissions":[
    "storage",
    "activeTab",
    "scripting"
  ],
  "action":{
    "default_popup":"mainPopup.html",
    "default_icon":{
      "16":"/images/icon_ex16.png",
      "32":"/images/icon_ex32.png",
      "48":"/images/icon_ex48.png",
      "128":"/images/icon_ex128.png"
    }
  },
  "icons":{
    "16":"/images/icon_ex16.png",
    "32":"/images/icon_ex32.png",
    "48":"/images/icon_ex48.png",
    "128":"/images/icon_ex128.png"
  }
}